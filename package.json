{
  "name": "coderonin-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon backend/server.js",
    "start": "node backend/server.js",
    "seed": "node backend/src/db/seed-run.js",
    "build": "node -e \"import{rmSync,cpSync}from'fs';import{resolve}from'path';const src=resolve('frontend','public');const dst=resolve('dist');rmSync(dst,{recursive:true,force:true});cpSync(src,dst,{recursive:true});console.log('Static build ready at',dst);\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
